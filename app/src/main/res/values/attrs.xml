        <attr name="includeStatusBar" format="boolean"/>

        <attr name="backgroundRes" format="reference"/>

        <attr name="showBack" format="boolean"/>
        <attr name="backImg" format="reference"/>

        <attr name="titleStr" format="string"/>
        <attr name="titleColor" format="color"/>
